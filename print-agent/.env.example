# Backend URL (change to your cloud URL if deployed)
BACKEND_URL=http://localhost:3000

# API Token for authentication (get this from your backend admin panel)
API_TOKEN=your-api-token-here

# Agent ID (unique identifier for this agent)
AGENT_ID=restaurant-main-agent

# Poll interval in milliseconds (how often to check for new jobs)
POLL_INTERVAL=3000

# Maximum retry attempts for failed jobs
MAX_RETRIES=3
